{
  "manifest_version": 2,
  "name": "sfw-helper",
  "version": "0.1",
  "description":"filters content from web pages",

  // "browser_action": {
  //   "default_icon": "icon.png"
  // },
  "author": "Matthew Ham, Jenny Wu, Eric Rabiner, Fardeen Chowdhury",
  "permissions": [
    "https://some-random-api.ml/*"
  ],
  "background": {
    "scripts":["./js/background.js"],
    "persistent": false
  },
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["./js/jquery-3.4.1.min.js", "./js/tf-1.0.0.min.js", "./js/mobilenet-1.0.0.js", "./js/content.js", "./js/ml.js"]

  }
]
}